generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model IFile {
  id          String  @id @default(cuid())
  description String?
  name        String
  url         String
  size        Int?
}
